{
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    {
      "source": "/(.*)",
      "has": [
        {
          "type": "host",
          "value": "www.rajughoshbd.com"
        }
      ],
      "destination": "https://rajughoshbd.com/:1",
      "permanent": true
    }
  ]
}
